{
    "name": "backpack/medialibrary-uploaders",
    "description": "Helper functions to save files with spatie media library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tabacitu",
            "email": "hello@backpackforlaravel.com",
            "homepage": "https://backpackforlaravel.com"
        }
    ],
    "homepage": "https://github.com/backpack/medialibrary-uploaders",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "spatie medialibrary uploaders"
    ],
    "require": {
        "backpack/crud": "dev-fix-uploaders as 6.7",
        "spatie/laravel-medialibrary": "^10.7|^11.3"
    },
    "require-dev": {
       "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\MediaLibraryUploaders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\MediaLibraryUploaders\\Tests\\": "tests",
            "Backpack\\CRUD\\Tests\\": "vendor/backpack/crud/tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backpack\\MediaLibraryUploaders\\AddonServiceProvider"
            ]
        }
    }
}
